                                    var id = ViewBag.userNomPre[a].id;
                                    var nom_pren = ViewBag.userNomPre[a].nom.ToString() + " " + ViewBag.userNomPre[a].prenom.ToString();
                                    if (ViewBag.tmpSup == id)
