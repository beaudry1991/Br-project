                            <select class="select2_single form-control" tabindex="-1" name="type_emb" id="type_emb">

                               

                                @if (ViewBag.tmpEmb == null)
                                {
                                    <option value="Regulier">Regulier</option>
                                    <option value="Exclusif">Exclusif</option>
                                    <option value="COOP">COOP</option>
                                    <option value="Autres">Autres</option>

                                }
                                else {

                                    <option value="@ViewBag.tmpEmb" selected>@ViewBag.tmpEmb</option>
