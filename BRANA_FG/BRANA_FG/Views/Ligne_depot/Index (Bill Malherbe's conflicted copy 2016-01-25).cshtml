                   <input autocomplete='off' class='filter form-control' name='@Html.DisplayNameFor(model => model.id_ligne_production)' placeholder='Ligne prod.' data-col='@Html.DisplayNameFor(model => model.id_ligne_production)' />
                     <input autocomplete='off' class='filter form-control' name='@Html.DisplayNameFor(model => model.date_ligne_depot)' placeholder='Date' data-col='@Html.DisplayNameFor(model => model.date_ligne_depot)' />
                    
                    <input autocomplete='off' class='filter form-control' name='@Html.DisplayNameFor(model => model.id_depot)' placeholder='Depot' data-col='@Html.DisplayNameFor(model => model.id_depot)' />
                    <input autocomplete='off' class='filter form-control' name='@Html.DisplayNameFor(model => model.id_produit)' placeholder='Produit' data-col='@Html.DisplayNameFor(model => model.id_produit)' />
                </div>
                <!-- <div class="form-group col-md-3 col-sm-4 col-xs-6">
