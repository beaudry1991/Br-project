                <script>
                                                                    document.write("<a href= '?graphique=ssss' class='btn btn-success navbar-right'>Repport to excel</a>");
                </script>
                <div class="clearfix"></div>
            </div>
            <div class="row">

                <table id="example" class="table table-striped responsive-utilities jambo_table">
                    <thead>
                        <tr class="headings">
                            <th>
                                @Html.DisplayNameFor(model => model.id_ligne_production)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.id_depot)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.qtite_caisse)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.date_ligne_depot)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.id_superviseur)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.id_data_clock)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.id_produit)
                            </th>
                            <th></th>
                        </tr>
                       
                    </thead>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr class="even pointer">
                                <td>
                                    @Html.DisplayFor(modelItem => item.id_ligne_production)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.id_depot)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.qtite_caisse)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.date_ligne_depot)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.id_superviseur)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.id_data_clock)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.id_produit)
                                </td>
                                <td>
                                    @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                                </td>
                            </tr>
                        }
                     

                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>
