
@{
    ViewBag.Title = "AcceuilSup";
}

<h2>AcceuilSup</h2>

<div class="">

    <div class="row top_tiles">
        <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="tile-stats">
                <div class="icon">
                    <i class="fa fa-caret-square-o-right"></i>
                </div>
                <div class="count">100</div>

                <h3>Loadings</h3>
                <p>12/12/2016</p>
            </div>
        </div>
        <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="tile-stats">
                <div class="icon">
                    <i class="fa fa-comments-o"></i>
                </div>
                <div class="count">3</div>

                <h3>Short fill</h3>
                <p>12/12/2016</p>
            </div>
        </div>
        <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="tile-stats">
                <div class="icon">
                    <i class="fa fa-sort-amount-desc"></i>
                </div>
                <div class="count">5</div>

                <h3>Casse</h3>
                <p>12/12/2016</p>
            </div>
        </div>
        <div class="animated flipInY col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="tile-stats">
                <div class="icon">
                    <i class="fa fa-check-square-o"></i>
                </div>
                <div class="count">9</div>

                <h3>Perte</h3>
                <p>12/12/2016</p>
            </div>

        </div>


    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">

                <div class="x_content">
                    <div class="col-md-9 col-sm-12 col-xs-12">
                        <div class="demo-container">
                            <div class="row">

                                <div class="col-md-12">
                                    <table id="example" class="table table-striped responsive-utilities ">
                                        <thead>
                                            <tr class="headings" bgcolor="#3A3F4F">

                                                <th>sku</th>
                                                <th>Stock  dispo.</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @for (var i = 0; i < ViewBag.sizeP; i++)
                                            {
                                                var nomprod = ViewBag.produitlist[i].nom.ToString();
                                                <tr class="even pointer">
                                                    <td class="btn-success">@nomprod</td>
                                                    <td class=" ">@ViewBag.listproddispo[i].caisse   @ViewBag.listproddispo[i].bouteille</td>
                                                </tr>
                                            }



                                        </tbody>
                                    </table>
                                </div>
                             
                                <a href="#"> View All</a>

                            </div>
                        </div>


                    </div>

                    <div class="col-md-3 col-sm-12 col-xs-12">
                        <div>
                            <div class="x_title">
                                <h2>Catégories de produits</h2>

                                <div class="clearfix"></div>
                            </div>
                            
                              
                                    <button class="btn btn-danger col-md-12">Brassé</button>
                                    <button class="btn btn-primary col-md-12">Carbonaté</button>
                                    <button class="btn btn-success col-md-12">Energetique</button>
                                    <button class="btn btn-warning col-md-12">Eau</button>


                          
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
       





        
</div>

